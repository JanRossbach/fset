MACHINE test
SETS A={A1,A2}; B={B1,B2}
CONSTANTS x
PROPERTIES
x={(A2,B2),(A1,B1)}
VARIABLES yA1B2, yA1B1, yA2B2, yA2B1
INVARIANT yA1B2:BOOL & yA1B1:BOOL & yA2B2:BOOL & yA2B1:BOOL & IF yA1B2=TRUE THEN {(A1,B2)} ELSE {} END\/IF yA1B1=TRUE THEN {(A1,B1)} ELSE {} END\/IF yA2B2=TRUE THEN {(A2,B2)} ELSE {} END\/IF yA2B1=TRUE THEN {(A2,B1)} ELSE {} END:A+->>B
INITIALISATION yA1B2 := bool((A1,B2):x) || yA1B1 := bool((A1,B1):x) || yA2B2 := bool((A2,B2):x) || yA2B1 := bool((A2,B1):x)
END